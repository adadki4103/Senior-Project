<?xml version="1.0" encoding="UTF-8"?>

<?import com.gluonhq.charm.glisten.control.Icon?>
<?import com.gluonhq.charm.glisten.mvc.View?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<View fx:id="primary" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.KudosApp.views.ThirdPresenter">
   <center>
      <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="100.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="label" alignment="TOP_CENTER" text="Registration">
               <font>
                  <Font size="16.0" />
               </font></Label>
            <TextField fx:id="userNameField" alignment="TOP_CENTER" maxWidth="250.00" promptText="User Name" style="-fx-prompt-text-fill: #000000;">
               <font>
                  <Font size="16.0" />
               </font></TextField>
            <PasswordField fx:id="passWordField" alignment="TOP_CENTER" maxWidth="250.00" promptText="Password" style="-fx-prompt-text-fill: #000000;">
               <font>
                  <Font size="16.0" />
               </font></PasswordField>
            <Button fx:id="regSend" mnemonicParsing="false" onAction="#buttonClick" text="Register Send">
               <graphic>
                  <Icon content="LANGUAGE" />
               </graphic>
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Label fx:id="testLabel" text="User Names Must Be Unique">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Button fx:id="regGoBack" alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#goBack" text="Go Back">
               <graphic>
                  <Icon content="LANGUAGE" />
               </graphic>
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </center>
</View>
